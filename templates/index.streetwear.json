{
    "sections": {
        "announcement_bar": {
            "type": "announcement-bar",
            "blocks": {
                "announcement_1": {
                    "type": "announcement",
                    "settings": {
                        "text": "t:templates.index_streetwear.ticker.stickers",
                        "icon": "gift"
                    }
                },
                "announcement_2": {
                    "type": "announcement",
                    "settings": {
                        "text": "t:templates.index_streetwear.ticker.collab",
                        "link": "shopify://collections/collaborations",
                        "icon": "star"
                    }
                }
            },
            "block_order": [
                "announcement_1",
                "announcement_2"
            ],
            "settings": {
                "background_color": "#000000",
                "text_color": "#00ff00",
                "autoplay_speed": 3,
                "show_close": false
            }
        },
        "slideshow": {
            "type": "slideshow",
            "blocks": {
                "slide_1": {
                    "type": "slide",
                    "settings": {
                        "subheading": "SS25 Drop",
                        "heading": "t:templates.index_streetwear.hero.slide_1_heading",
                        "text": "t:templates.index_streetwear.hero.slide_1_text",
                        "button_label": "Shop Now",
                        "button_link": "shopify://collections/new-arrivals",
                        "button_label_2": "Lookbook",
                        "button_link_2": "shopify://pages/lookbook",
                        "overlay_opacity": 40
                    }
                },
                "slide_2": {
                    "type": "slide",
                    "settings": {
                        "subheading": "Collab Alert",
                        "heading": "t:templates.index_streetwear.hero.slide_2_heading",
                        "text": "t:templates.index_streetwear.hero.slide_2_text",
                        "button_label": "View Collab",
                        "button_link": "shopify://collections/collaborations",
                        "overlay_opacity": 50
                    }
                },
                "slide_3": {
                    "type": "slide",
                    "settings": {
                        "subheading": "Member Only",
                        "heading": "t:templates.index_streetwear.hero.slide_3_heading",
                        "text": "t:templates.index_streetwear.hero.slide_3_text",
                        "button_label": "Join Crew",
                        "button_link": "shopify://pages/membership",
                        "overlay_opacity": 50
                    }
                }
            },
            "block_order": [
                "slide_1",
                "slide_2",
                "slide_3"
            ],
            "settings": {
                "autoplay": true,
                "autoplay_speed": 4,
                "show_arrows": true,
                "show_dots": true,
                "slide_height": "large"
            }
        },
        "featured_collection": {
            "type": "featured-collection",
            "settings": {
                "title": "Latest Drop",
                "description": "Fresh off the press. Limited quantities.",
                "layout": "carousel",
                "products_to_show": 10,
                "columns": 4,
                "show_vendor": true,
                "show_swatches": true,
                "show_second_image": true,
                "show_quick_view": true,
                "show_view_all": true,
                "button_text": "See All",
                "button_style": "primary"
            }
        },
        "collection_list": {
            "type": "collection-list",
            "blocks": {
                "collection_1": {
                    "type": "collection",
                    "settings": {
                        "title": "Tees"
                    }
                },
                "collection_2": {
                    "type": "collection",
                    "settings": {
                        "title": "Hoodies"
                    }
                },
                "collection_3": {
                    "type": "collection",
                    "settings": {
                        "title": "Pants"
                    }
                },
                "collection_4": {
                    "type": "collection",
                    "settings": {
                        "title": "Accessories"
                    }
                }
            },
            "block_order": [
                "collection_1",
                "collection_2",
                "collection_3",
                "collection_4"
            ],
            "settings": {
                "title": "Categories",
                "columns": 4,
                "image_ratio": "square",
                "show_view_all": true,
                "button_text": "All Categories"
            }
        },
        "image_with_text": {
            "type": "image-with-text",
            "settings": {
                "image_position": "left",
                "image_ratio": "portrait",
                "eyebrow": "Our Vibe",
                "title": "Not For Everyone",
                "text": "t:templates.index_streetwear.image_with_text.text",
                "button_text": "About Us",
                "button_url": "shopify://pages/about",
                "button_style": "secondary",
                "background_color": "secondary"
            }
        },
        "testimonials": {
            "type": "testimonials",
            "blocks": {
                "testimonial_1": {
                    "type": "testimonial",
                    "settings": {
                        "name": "@hypebeast_tony",
                        "title": "Instagram",
                        "rating": 5,
                        "text": "t:templates.index_streetwear.testimonials.testimonial_1"
                    }
                },
                "testimonial_2": {
                    "type": "testimonial",
                    "settings": {
                        "name": "@streetstyle_maria",
                        "title": "Instagram",
                        "rating": 5,
                        "text": "t:templates.index_streetwear.testimonials.testimonial_2"
                    }
                }
            },
            "block_order": [
                "testimonial_1",
                "testimonial_2"
            ],
            "settings": {
                "heading": "t:templates.index_streetwear.testimonials.heading",
                "subheading": "@streetwear"
            }
        },
        "recently_viewed": {
            "type": "recently-viewed",
            "settings": {
                "heading": "t:templates.index_streetwear.recently_viewed.heading",
                "products_to_show": 4,
                "columns": 4
            }
        },
        "newsletter": {
            "type": "rich-text",
            "settings": {
                "background_color": "accent",
                "text_alignment": "center",
                "narrow_content": true,
                "eyebrow": "Don't Sleep",
                "title": "Join the Inner Circle",
                "text": "t:templates.index_streetwear.newsletter.text",
                "show_newsletter": true
            }
        }
    },
    "order": [
        "announcement_bar",
        "slideshow",
        "featured_collection",
        "collection_list",
        "image_with_text",
        "testimonials",
        "recently_viewed",
        "newsletter"
    ]
}