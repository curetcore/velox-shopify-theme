{
    "sections": {
        "hero": {
            "type": "hero-simple",
            "settings": {
                "eyebrow": "Help Center",
                "title": "Frequently Asked Questions",
                "text": "t:templates.page_faq.header.text",
                "text_position": "center"
            }
        },
        "faq_shipping": {
            "type": "faq",
            "blocks": {
                "q1": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.shipping.time_question",
                        "answer": "t:templates.page_faq.shipping.time_answer",
                        "open_by_default": true
                    }
                },
                "q2": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.shipping.free_question",
                        "answer": "t:templates.page_faq.shipping.free_answer"
                    }
                },
                "q3": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.shipping.tracking_question",
                        "answer": "t:templates.page_faq.shipping.tracking_answer"
                    }
                }
            },
            "block_order": [
                "q1",
                "q2",
                "q3"
            ],
            "settings": {
                "heading": "t:templates.page_faq.shipping.heading"
            }
        },
        "faq_returns": {
            "type": "faq",
            "blocks": {
                "q1": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.returns.policy_question",
                        "answer": "t:templates.page_faq.returns.policy_answer"
                    }
                },
                "q2": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.returns.start_question",
                        "answer": "t:templates.page_faq.returns.start_answer"
                    }
                },
                "q3": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.returns.refund_question",
                        "answer": "t:templates.page_faq.returns.refund_answer"
                    }
                }
            },
            "block_order": [
                "q1",
                "q2",
                "q3"
            ],
            "settings": {
                "heading": "t:templates.page_faq.returns.heading"
            }
        },
        "faq_products": {
            "type": "faq",
            "blocks": {
                "q1": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.products.size_question",
                        "answer": "t:templates.page_faq.products.size_answer"
                    }
                },
                "q2": {
                    "type": "question",
                    "settings": {
                        "question": "t:templates.page_faq.products.authentic_question",
                        "answer": "t:templates.page_faq.products.authentic_answer"
                    }
                }
            },
            "block_order": [
                "q1",
                "q2"
            ],
            "settings": {
                "heading": "t:templates.page_faq.products.heading"
            }
        },
        "contact_cta": {
            "type": "rich-text",
            "settings": {
                "background_color": "secondary",
                "text_alignment": "center",
                "narrow_content": true,
                "title": "Still Have Questions?",
                "text": "t:templates.page_faq.footer.text",
                "button_text": "Contact Us",
                "button_url": "shopify://pages/contact",
                "button_style": "primary"
            }
        }
    },
    "order": [
        "hero",
        "faq_shipping",
        "faq_returns",
        "faq_products",
        "contact_cta"
    ]
}
